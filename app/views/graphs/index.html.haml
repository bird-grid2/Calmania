.wrapper
  .side-management
    = link_to managements_path do
      = icon('fas', 'home')
      %h6.pc Home
      %h6.sp Home
    = link_to destroy_user_session_path, method: :delete do
      = icon('fas', 'sign-out-alt')
      %h6.pc Sign<br>out
      %h6.sp Sign out
  .graph_wrapper
    .graph_upper
      .left_graph
        = image_tag "/assets/test_#{@user.id}_2.png"
      .right_graph
        = image_tag "/assets/test_#{@user.id}_4.png"
    .graph_bottom
      .left_graph
        = image_tag "/assets/test_#{@user.id}_6.png"
      .right_graph
        = image_tag "/assets/test_#{@user.id}_8.png"