.wrapper
  .upper-info
    %table
      %tr
        %th.table-title Date
        %th.table-title Menu
        %th.table-title Weight
        %th.table-title BFP
        %th.table-title Total Cal
      %tr
        %td.table-item
        -#  = @logs.created_at
        %td.table-item
